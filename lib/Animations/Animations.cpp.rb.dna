a.first.attribute_sections.each do |section|
  name    = a.func_array_name(section)
  content = a.map(&:"#{section}_attribute").join(',')
  typ     = a.class.attributes[section]
.PROGMEM const @{typ} @{name}[] = {@{content}};
end

